{
  "version": 2,
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "destination": "/ro",
      "statusCode": 302
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ],
  "trailingSlash": false
} 